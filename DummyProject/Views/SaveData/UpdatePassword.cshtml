<script src="~/Scripts/ui-bootstrap-tpls-0.10.0.js"></script>
<script src="~/Scripts/ui-bootstrap-tpls.js"></script>
<script src="~/Content/js/moment.js"></script>
<script src="~/Content/js/bootstrap-datetimepicker.js"></script>
