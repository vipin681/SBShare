
@{
    ViewBag.Title = "DashBoard";
}

<h2>DashBoard</h2>

<script src="~/Scripts/aes.js"></script>
<script src="~/Content/js/jquery-1.9.1.js"></script>
<script src="~/Scripts/angular.js"></script>

<script src="~/Content/js/bootstrap.js"></script>
